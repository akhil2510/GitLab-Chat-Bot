{
  "name": "gitlab-ai-chatbot",
  "version": "1.0.0",
  "type": "module",
  "description": "GenAI Chatbot for GitLab Handbook and Direction Pages",
  "scripts": {
    "setup": "./setup.sh",
    "install:all": "cd backend && npm install && cd ../frontend && pip3 install -r requirements.txt",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && streamlit run app.py",
    "scrape": "cd backend && npm run scrape",
    "index": "cd backend && npm run index",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint:backend": "cd backend && npm run lint",
    "test:backend": "cd backend && npm test"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "@huggingface/inference": "^4.13.0",
    "@pinecone-database/pinecone": "^2.0.0",
    "axios": "^1.5.1",
    "cheerio": "1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.4",
    "node-cache": "^5.1.2",
    "winston": "^3.11.0"
  },
  "keywords": [
    "chatbot",
    "genai",
    "gitlab",
    "rag",
    "ai",
    "llm",
    "vector-search",
    "streamlit",
    "node.js"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/gitlab-ai-chatbot"
  }
}
