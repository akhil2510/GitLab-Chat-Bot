{
  "name": "gitlab-chatbot-backend",
  "version": "1.0.0",
  "description": "GenAI Chatbot Backend for GitLab Handbook and Direction Pages",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "setup:pinecone": "node src/scripts/createPineconeIndex.js",
    "scrape": "node src/scripts/scrapeGitLabData.js",
    "index": "node src/scripts/indexData.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "chatbot",
    "genai",
    "gitlab",
    "rag",
    "vector-search"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "@huggingface/inference": "^4.13.0",
    "@pinecone-database/pinecone": "^2.0.0",
    "axios": "^1.5.1",
    "cheerio": "1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.4",
    "node-cache": "^5.1.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  }
}
